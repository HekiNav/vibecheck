{
    "manifest_version": 3,
    "name": "VibeCheck",
    "version": "1.0",
    "description": "fancy description here",
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": ["*://*.youtube.com/*", "*://*.youtube.com/*"],
            "js": [
                "content.js"
            ],
            "css": [
                "style.css"
            ]
        }
    ]
}